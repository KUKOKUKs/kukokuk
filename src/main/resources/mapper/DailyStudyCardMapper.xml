<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kukokuk.mapper.DailyStudyCardMapper">

  <!--
    void insertDailyStudyCard(DailyStudyCard dailyStudyCard);
  -->
  <insert id="insertDailyStudyCard" parameterType="DailyStudyCard">
    INSERT INTO KUKOKUK_DAILY_STUDY_CARDS
      (DAILY_STUDY_NO, CARD_INDEX, TITLE, CONTENT)
    VALUES
      (#{dailyStudyNo}, #{cardIndex}, #{title}, #{content})
  </insert>


</mapper>